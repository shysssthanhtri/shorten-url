  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "export NODE_ENV=production; node dist/main",
    "start:local-env": "docker compose -f ./docker/docker-compose.yml up",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export NODE_ENV=test-local; jest --config ./test/jest.json",
    "test:watch": "export NODE_ENV=test-local; jest --config ./test/jest.json --watch",
    "test:e2e": "export NODE_ENV=test-local; jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/supertest": "^2.0.11",
    "jest": "29.3.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3"
  }

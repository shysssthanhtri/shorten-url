{
  "name": "user-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "-----START---------------------------------------": "",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "export NODE_ENV=production; node dist/main",
    "start:local-env": "docker compose -f ./docker/docker-compose.yml up",
    "-----TEST---------------------------------------": "",
    "test": "export NODE_ENV=test-local; jest --config ./test/jest.json",
    "test:watch": "export NODE_ENV=test-local; jest --config ./test/jest.json --watch",
    "test:e2e": "export NODE_ENV=test-local; jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "export NODE_ENV=test-local; jest --config ./test/jest-e2e.json --watch",
    "-----MIGRATION---------------------------------------": "",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js",
    "migration:show": "pnpm typeorm migration:show --dataSource src/ormconfig.ts",
    "migration:generate": "pnpm typeorm --dataSource src/ormconfig.ts migration:generate",
    "migration:create": "pnpm typeorm migration:create ./migrations/$npm_config_name",
    "migration:run": "pnpm typeorm migration:run --dataSource src/ormconfig.ts",
    "migration:revert": "pnpm typeorm --dataSource src/ormconfig.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "@shorten-url/base-service": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/supertest": "^2.0.11",
    "jest": "29.3.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3"
  }
}